input {
  udp {
    port => 5000
    type => json
  }
}

filter {
  if [type] == "docker" {
    json {
      source => "message"
    }
    mutate {
      rename => [ "log", "message" ]
    }
    date {
      match => [ "time", "ISO8601" ]
    }
  }
}

output {
    elasticsearch {
    host => "127.0.0.1"
    port => "9200"
    protocol => "http"
  }
  #stdout { codec => rubydebug }
}
