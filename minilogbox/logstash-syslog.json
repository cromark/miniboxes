input {
  udp { port => 5000 codec => json }
}

filter {

  if [type] == "json" {
    json {
      source => "message"
    }
    mutate {
      rename => [ "log", "message" ]
    }
    date {
      match => [ "time", "ISO8601" ]
    }
  }
}

output {
  stdout {
    codec => rubydebug
  }

  elasticsearch {
    host => "127.0.0.1"
    port => "9200"
    protocol => "http"
  }
}
