input {
  udp { port => 5000 type => "logstash" }
}

filter {

  if [type] == "logstash" {
    json {
      source => "message"
    }

    date {
      match => [ "time", "ISO8601" ]
    }
  }
}

output {
  stdout {
    codec => rubydebug
  }

  elasticsearch {
    host => "127.0.0.1"
    port => "9200"
    protocol => "http"
  }
}
